import java.util.Scanner;
import java.util.Arrays;
class TwoDArray
{

 public static void main(String args[])
{
 Scanner input=new Scanner(System.in);
 int r=input.nextInt();
 System.out.println("\nEnter the number of rows you want " + r);
 int c=input.nextInt();
 System.out.println("\nEnter the number of columns you want " + c);
   int array[][]=new int[r][c];
  
  for(int i=0;i<r;i++)
  {
    for(int j=0;j<c;j++)
     {
       int m=input.nextInt();
       int n=input.nextInt();
       System.out.println("array[i][j]"+array[m][n]);
     }
   }
}
}
